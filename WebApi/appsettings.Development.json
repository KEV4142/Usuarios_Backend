{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION}",
    "DefaultConnectionModificado": "${DB_CONNECTIONMODIFICADO}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "${BACKEND_ORIGIN}",
  "AllowedFrontendHosts": "${FRONTEND_ORIGIN}",
  "TokenKey": "${TOKEN_KEY}",
  "EmailSettings": {
    "SmtpServer": "${SMTPSERVER}",
    "Port": "${PORT_MAIL}",
    "SenderEmail": "${SENDEREMAIL}",
    "SenderName": "${SENDERNAME}",
    "Username": "${USERNAME}",
    "Password": "${PASSWORD}"
  }
}
